{
  "name": "grapesjs-sokeio",
  "version": "1.0.65",
  "description": "Grapesjs Shortcode",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sokeio/grapesjs-builder.git"
  },
  "scripts": {
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build",
